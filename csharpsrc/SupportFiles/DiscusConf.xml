<?xml version="1.0" encoding="utf-8" ?>
<Config>
	<GateKeeperName>LOC</GateKeeperName>
	<ProxyCache>C:\temp\PxyCache</ProxyCache>
	<!-- Logging config -->
	<Logging>
		<!-- "EventLog", "UrlLog" or "WebServiceLog" -->
		<LoggingType>UrlLog</LoggingType>
		<UrlLogging>
			<Hostname>127.0.0.1</Hostname>
			<Port>8080</Port>
		</UrlLogging>
		<WebServiceLogging>
			<WSDL>http://localhost/PSLGatekeeper1/PSLGatekeeper1.asmx?WSDL</WSDL>
			<AccessPoint>http://localhost/PSLGatekeeper1/PSLGatekeeper1.asmx</AccessPoint>
		</WebServiceLogging>
	</Logging>
	<!-- Tracing config -->
	<Tracing>
		<!-- "EventTrace", "UrlTrace" or "WebServiceTrace" -->
		<TraceOn>False</TraceOn>
		<TracingType>UrlTrace</TracingType>
		<UrlTracing>
			<Hostname>127.0.0.1</Hostname>
			<Port>8080</Port>
		</UrlTracing>
		<WebServiceTracing>
			<WSDL>http://localhost/PSLGatekeeper1/PSLGatekeeper1.asmx?WSDL</WSDL>
			<AccessPoint>http://localhost/PSLGatekeeper1/PSLGatekeeper1.asmx</AccessPoint>
		</WebServiceTracing>
	</Tracing>
</Config>